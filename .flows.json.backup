        "type": "functionx",
        "z": "4f19d12f.1e2c4",
        "name": "s-mode decode",
        "func": "const Decoder = require('mode-s-decoder')\n\nlet decoder = flow.get('decoder')\nif(!decoder){\n    decoder = new Decoder();\n    flow.set('decoder', decoder)\n}\nmsg.payload = decoder.parse(msg.payload);\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 200,
