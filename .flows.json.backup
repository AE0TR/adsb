    {
        "id": "15c640ed79dd6ac0",
        "type": "function",
        "z": "4f19d12f.1e2c4",
        "name": "hex->bin",
        "func": "function parseHexString(str) { \n    var result = [];\n    while (str.length > 0) { \n        result.push(parseInt(str.substring(0, 2), 8));\n        str = str.substring(2, str.length);\n    }\n\n    return result;\n}\n\nmsg.payload = parseHexString(msg.payload);\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 600,
        "y": 140,
